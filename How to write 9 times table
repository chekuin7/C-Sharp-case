       private void button1_Click(object sender, EventArgs e)
       {
           string result = "";
           for(int i = 1;i <=9; i++)
           {
               for(int j = 1;j <= 9; j++)
               {
                   result += string.Format("{0} * {1} = {2:00}  ", i, j, i*j);
               }
           result += "\r\n";
           }
           textBox1.Text = result;
       }
#{2:00} means every value should be written in the 2-digit way.
